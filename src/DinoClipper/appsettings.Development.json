{
  "ConnectionStrings": {
    "litedb": "./data/dino.db"
  },
  "DinoClipper": {
    "TempStorage": "./data/tmp",
    "FfmpegPath": "/opt/homebrew/bin",
    "YouTubeDlPath": "/opt/homebrew/bin/yt-dlp",
    "UploadTarget": {
      "Type": "LocalFileSystem",
      "Url": "./data/clips"
    },
    "DownloaderFlags": {
      "MaxWorkerThreads": 1,
      "UploadOriginal": true,
      "SkipUpload": false,
      "SkipClearingTempDirectory": false
    }
  }
}